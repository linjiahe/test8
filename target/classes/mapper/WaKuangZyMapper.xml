<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.project.mapper.WaKuangZyMapper">
    <select id="selectNotReturnByUserIdsAndCoin" resultType="com.company.project.entity.WaKuangZy">
        select * from wa_kuang_zy zy where zy.coin = #{coin} and zy.is_return=0 and
        <foreach collection="list" item="i" index="index" open="(" separator="or" close=")">
            zy.user_id=#{i.userId}
        </foreach>
    </select>
</mapper>
